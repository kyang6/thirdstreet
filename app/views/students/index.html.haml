.container
  %h1 Listing students

  %table
    %thead
      %tr
        %th Name
        
   
    %tbody
      - @students.each do |student|
        -if current_user.id == student.user_id
          %tr
            %td=  link_to student.name, student
       

  %br

  = link_to 'New Student', new_student_path
